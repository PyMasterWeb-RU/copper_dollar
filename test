<script>
    $(function () {
        let $datepickerCurrency = $datepicker = $("#datepicker_currency");

        setTimeout(function () {
            $datepicker = $("#datepicker_currency").data("datepickerElement");
            $datepicker.datepicker("option", { dateFormat: "dd.mm.yy", changeMonth: true, changeYear: true, minDate: "01.07.1992", maxDate: "27.05.2023" });

            $datepickerCurrency.on("filter-select", function () {
            var rate,
                tab = "1",
                num = "840",
                currentDate = $.datepicker.formatDate("yy-mm-dd", $(this).datepicker("getDate")),
                $span = $(".currency_price span[data-currency]"),
                $currency = $(".currency");

            $span.each(function () {
                $(this).html("\u2014");
            });
            $currency.addClass("_loading");

            $.getJSON("/ref/search/currency-wizard/" + tab + "/" + num + "/" + currentDate, function (data) {
                if (data.length) {
                    $span.empty();
                    $.each(data, function (key, val) {
                        rate = val.units + "/" + val.rate.toFixed(4).replace(".", ",");
                        $(".currency_price span[data-currency='" + val.id + "']").text(rate);
                    });
                }
            }).always(function () {
                $currency.removeClass("_loading");
            });
        })}, 0);
    });
</script>            </div>
        </div>
