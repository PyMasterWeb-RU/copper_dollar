<script type="text/javascript">
    $(function () {
        var $form = $('form:has(div.search)');
        function searchFormSubmit() {
            $form.find('button').attr('disabled', true);
        };
        $form.submit(searchFormSubmit);

        $('.cross-result:not(:has(._with-icon))').each(function () {
            var $link = $(this).find('.title-source a');
            var href = $link.prop('href');

            var words = {};
            $(this).find('.title-source mark').each(function () {
                var parts = $(this).text().split(/[^a-zа-яё0-9_\-]+/gi);

                for (var i = 0; i < parts.length; ++i) {
                    var word = parts[i].toLowerCase();
                    if (word.length > 3) {
                        words[word] = true;
                    }
                }
            });

            var str = '';
            for (var word in words) {
                if (str !== '') str += '|';
                str += word;
            }

            if (str !== '') {
                $link.prop('href', href + '#highlight=' + encodeURIComponent(str));
            }
        });
    });
</script>

